	i2c_ids {
		compatible = "msc,i2c-ids";
		i2c_gp {
			label = "gp";
			bus = <&i2c1>;
		};
		i2c_pm {
			label = "pm";
			bus = <&i2c2>;
		};
		i2c_lcd {
			label = "lcd";
			bus = <&i2c3>;
		};
		i2c_cam0 {
			label = "cam0";
			bus = <&i2c4>;
		};
		i2c_cam1 {
			label = "cam1";
			bus = <&i2c5>;
		};
		i2c_dev {
			label = "dev";
			bus = <&i2c6>;

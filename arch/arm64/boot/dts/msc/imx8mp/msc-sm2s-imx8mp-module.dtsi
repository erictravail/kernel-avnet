&aud2htx {
	status = "disabled";
};

&A53_0 {
	cpu-supply = <&vcc_arm>;
};

&A53_1 {
	cpu-supply = <&vcc_arm>;
};

&A53_2 {
	cpu-supply = <&vcc_arm>;
};

&A53_3 {
	cpu-supply = <&vcc_arm>;
